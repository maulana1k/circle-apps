{
    "buildCommand":"yarn build",
    "devCommand" : "yarn dev",
    "installCommand":"yarn install",
    "outputDirectory":"dist/apps/react",
    "builds":[
        {
            "src":"dist/apps/api/main.js",
            "use":"@vercel/node",
            "config":{"includeFiles":["dist/apps/api/**"]}
        },
        {
            "src":"dist/apps/react/**",
            "use":"@vercel/static",
            "config":{"includeFiles":["dist/apps/react/**"]}

        }
    ],
    "routes":[
        {
            "src":"/api/(.*)",
            "dest":"dist/apps/api/main.js"
        },
        {
            "src":"/(.*)",
            "dest":"dist/apps/react/**"
        }
    ]
}