{
    "buildCommand":"yarn build",
    "devCommand" : "yarn dev",
    "installCommand":"yarn install",
    "outputDirectory":"dist",
    "builds":[
        {
            "src":"./dist/apps/main.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"dist/apps/api/main.js"
        }
    ]
}